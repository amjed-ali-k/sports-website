{
  "name": "sports-management",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "generate": "turbo db:generate --filter @sports/api",
    "migrate": "turbo db:migrate --filter @sports/api",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/node": "^20.17.9",
    "prettier": "^3.4.1",
    "turbo": "^2.3.3",
    "typescript": "^5.3.3"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "bun@1.1.38",
  "dependencies": {
    "@radix-ui/react-progress": "^1.1.1"
  }
}
